{
  "name": "ctrlv",
  "version": "1.0.0",
  "description": "Your cloud clipboard",
  "main": "server.js",
  "scripts": {
    "nyc": "nyc report",
    "start": "node server.js",
    "test": "nyc mocha",
    "test-watch": "nodemon --exec npm test"
  },
  "author": "Lucas Czekaj",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.18.3",
    "express": "^4.16.4",
    "mongodb": "^3.1.10",
    "mongoose": "^5.4.2"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-date-string": "^0.1.0",
    "mocha": "^5.2.0",
    "mongodb-memory-server": "^2.9.1",
    "morgan": "^1.9.1",
    "nodemon": "^1.18.9",
    "nyc": "^13.1.0",
    "standard": "^12.0.1",
    "supertest": "^3.3.0"
  },
  "standard": {
    "globals": [
      "describe",
      "it",
      "beforeEach",
      "before",
      "after"
    ]
  },
  "nyc": {
    "extension": [
      ".js"
    ],
    "reporter": [
      "text",
      "html"
    ],
    "all": true,
    "check-coverage": true,
    "statements": 90,
    "functions": 90,
    "branches": 90,
    "lines": 90
  }
}
